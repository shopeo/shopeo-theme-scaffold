{
	"name": "shopeo-theme-scaffold",
	"version": "0.0.1",
	"description": "SHOPEO Theme Scaffold",
	"scripts": {
		"dev": "npm run development",
		"development": "mix",
		"watch": "mix watch",
		"watch-poll": "mix watch -- --watch-options-poll=1000",
		"hot": "mix watch --hot",
		"prod": "npm run production",
		"production": "mix --production",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style 'src/assets/scss/**/*.scss'",
		"lint:js": "wp-scripts lint-js 'src/assets/js/**/*.js'",
		"makepot": "wp i18n make-pot --domain='shopeo-theme-scaffold' --include='assets/js,*.php' . --headers='{\"Report-Msgid-Bugs-To\":\"https://shopeo.cn/contact/\"}' --file-comment=\"Copyright (c) $(date +'%Y') SHOPEO. All Rights Reserved.\" languages/shopeo-theme-scaffold.pot",
		"packages-update": "wp-scripts packages-update"
	},
	"author": "SHOPEO",
	"homepage": "https://shopeo.cn",
	"license": "MIT",
	"devDependencies": {
		"@popperjs/core": "^2.11.8",
		"@wordpress/scripts": "^27.1.0",
		"autoprefixer": "^10.4.16",
		"bootstrap": "^5.3.2",
		"css-loader": "^6.8.1",
		"laravel-mix": "^6.0.49",
		"postcss": "^8.4.31",
		"postcss-import": "^15.1.0",
		"postcss-loader": "^7.3.3",
		"resolve-url-loader": "^5.0.0",
		"rtlcss": "^4.1.1",
		"sass": "^1.69.5",
		"sass-loader": "^13.3.2",
		"style-loader": "^3.3.3",
		"wp-readme-to-markdown": "^1.0.1"
	}
}
